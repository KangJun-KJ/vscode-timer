{
  "name": "vscode-timer",
  "displayName": "vscode-timer",
  "description": "",
  "version": "0.0.1",
  "publisher": "MaxfieldWalker",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.vscode-timer.reset",
        "title": "Reset timer",
        "category": "vscode-timer"
      },
      {
        "command": "extension.vscode-timer.setTimer",
